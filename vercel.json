{
    "builds": [{
        "src": "crop_predictor/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "crop_predictor/wsgi.py"
        }
    ],
    "rewrites": [{ "source": "/(.*)", "destination": "/" }]
}